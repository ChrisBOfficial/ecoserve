{
    "name": "invasive-species",
    "version": "0.1.0",
    "private": true,
    "description": "A platform for the integration of traditional and local knowledge with ecosystem service assessments and environmental decision making.",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "deploy": "npm run build && cd resources && ./prep-deploy.sh",
        "deploy-windows": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command npm run build; cd resources; ./prep-deploy.ps1",
        "devstart": "NODE_ENV=development nodemon src/server.js",
        "prestart": "npm install",
        "start": "node src/server.js",
        "test:unit": "vue-cli-service test:unit",
        "test:server": "mocha --exit",
        "test": "mocha --exit; vue-cli-service test:unit"
    },
    "main": "server.js",
    "dependencies": {
        "axios": "^0.19.1",
        "bootstrap": "^4.4.1",
        "bootstrap-vue": "^2.1.0",
        "cors": "^2.8.5",
        "d3": "^5.12.0",
        "d3-array": "2.3.3",
        "d3-scale": "3.2.0",
        "d3-selection": "1.4.0",
        "d3-transition": "1.2.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "mongodb": "^3.5.2",
        "request": "^2.88.0",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "unzip-stream": "^0.3.0",
        "vue": "^2.6.10",
        "vue-router": "^3.1.3",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-router": "^4.0.0",
        "@vue/cli-plugin-unit-jest": "^4.2.2",
        "@vue/cli-plugin-vuex": "^4.0.0",
        "@vue/cli-service": "^4.0.0",
        "@vue/test-utils": "1.0.0-beta.31",
        "babel-eslint": "^10.0.3",
        "eslint": "^6.6.0",
        "eslint-plugin-vue": "^6.0.1",
        "mocha": "^6.2.2",
        "nodemon": "^2.0.2",
        "supertest": "^4.0.2",
        "vue-template-compiler": "^2.6.10"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "bugs": {
        "url": "https://github.com/cseseniordesign/invasive-species/issues"
    },
    "homepage": "https://github.com/cseseniordesign/invasive-species#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cseseniordesign/invasive-species.git"
    }
}
